[user]
    name = nobu666
    email = nobutoshi.ogata@smartnews.com
[core]
    editor = vim
    autocrlf = false
    quotepath = false
    precomposeunicode = true
    quotepath = false
[color]
    ui = auto
    branch = auto
    diff = auto
    status = auto
    interactive = auto
    grep = auto
[pager]
    log = colordiff | less
    show = colordiff | less
    diff = colordiff | less
[merge]
    tool = vimdiff
    ff = false
[pull]
    rebase = true
[branch]
    autosetuprebase = always
[alias]
    sh  = show
    ls  = ls-files
    s   = status
    st  = status
    ss  = status -s
    ft  = fetch
    ftp = fetch --prune
    ci  = commit
    ca  = commit -a
    cv  = commit -v
    cav = commit -a -v
    cam = commit -a --amend
    co  = checkout
    cb  = checkout -b
    ct  = checkout --track
    cm  = checkout master
    cd  = checkout develop
    di  = diff
    dc  = diff --cached
    ds  = diff --staged
    dw  = diff -C --color-words
    dm  = diff master
    br  = branch
    up  = push
    pull = pull --rebase --no-edit
    mn  = merge --no-ff
    oneline = log --pretty=oneline
    logs = log --graph --decorate --pretty=format:'%ad [%cn] <c:%h t:%t p:%p> %n %Cgreen%d%Creset %s %n' --stat
    logall = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
    br = branch
    ba = branch -a
    bm = branch --merged
    bn = branch --no-merged
    gr = grep
    gn = grep -n
[ghq]
    root = ~/src
[credential]
    helper = osxkeychain
